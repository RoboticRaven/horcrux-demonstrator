2_temp \" + msg.matching.shareFolderPath + \"ibv\" + i + \"_share_2_decrypted_and_decoded\")\n    \n    // read as pure buffer\n    share1 = fs.readFileSync(msg.matching.shareFolderPath + \"ibv\" + i + \"_share_1_decrypted_and_decoded\")\n    share2 = fs.readFileSync(msg.matching.shareFolderPath + \"ibv\" + i + \"_share_2_decrypted_and_decoded\")\n    \n    // XOR shares\n    share_XORed = Buffer.alloc(share1.length)\n    for(var j = 0; j < share1.length; j++){\n        share_XORed[j] = share1[j] ^ share2[j]\n    }\n    \n    // REDUCE xored share\n    share_reduced = Buffer.alloc(share_XORed.length/2)\n    for(var k = 0; k < share_XORed.length / 2; k++){\n        let n = 2*k\n        let p = (2*k)+1\n        msbBuff[0] = share_XORed[n]\n        lsbBuff[0] = share_XORed[p]\n        reduced_msb = reduceBuffer(msbBuff)\n        reduced_lsb = reduceBuffer(lsbBuff)\n        let resBuffer = Buffer.alloc(1)\n        resBuffer[0] = resBuffer[0] | reduced_msb[0]\n        resBuffer[0] = resBuffer[0] << 4\n        resBuffer[0] = resBuffer[0] | reduced_lsb[0]\n        share_reduced[k] = resBuffer[0]\n    }\n}\n\nmsg.matching.reducedShare = share_reduced\nreturn msg;\n\nfunction reduceBuffer(inBuff){\n    let buff = inBuff\n    let res = Buffer.alloc(1)\n    for(var i = 0; i <= 3; i++){\n        let x = buff[0] & 0xc0\n        if(x == 0xc0){\n            res[0] += 1\n            buff[0] = buff[0] << 2\n            if(i != 3){\n                res[0] = res[0] << 1\n            }\n        }else{\n            buff[0] = buff[0] << 2\n            if(i != 3){\n                res[0] = res[0] << 1\n            }\n        }\n    }\n    return res\n}","outputs":1,"noerr":0,"initialize":"","finalize":"","x":200,"y":300,"wires":[["4cd50b19.4134f4","aadc8f7b.05bda","e1a6b843.c013f8"]]},{"id":"4cd50b19.4134f4","type":"function","z":"5b7be390.ca3074","name":"try matching","func":"msg.matching.status = false\nvar x = Buffer.compare(msg.data.ibvs[0].buffer,msg.matching.reducedShare)\nif (x ===